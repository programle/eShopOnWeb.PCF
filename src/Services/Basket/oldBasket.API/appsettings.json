{
  "SqlServer": {
    "credentials": {
      "ConnectionString": "Server=tcp:127.0.0.1,5433;Initial Catalog=Microsoft.eShopOnContainers.Services.Basket;User Id=sa;Password=Pass@word"
    }
  },
  "PicBaseUrl": "http://localhost:5101/api/v1/c/basket/items/[0]/pic/",
  "UseCustomizationData": false,
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Trace",
      "System": "Information",
      "Microsoft": "Information",
      "Pivotal": "Trace",
      "Steeltoe": "Trace"
    }
  },
  "AzureServiceBusEnabled": false,
  "AzureStorageEnabled": false,
  "SubscriptionClientName": "basket",
  "ApplicationInsights": {
    "InstrumentationKey": ""
  },
  "UseVault": false,
  "Vault": {
    "Name": "eshop",
    "ClientId": "your-clien-id",
    "ClientSecret": "your-client-secret"
  },
  "spring": {
    "application": {
      "name": "eshop-basket-api"
    }
  },
  "eureka": {
    "client": {
      "serviceUrl": "http://localhost:8761/eureka/",
      "shouldRegisterWithEureka": true,
      "shouldFetchRegistry": false,
      "ValidateCertificates": false
    },
    "instance": {
      "hostname": "localhost",
      "port": 5000,
      "statusPageUrlPath": "/cloudfoundryapplication/info",
      "healthCheckUrlPath": "/cloudfoundryapplication/health"
    }
  },
  "rabbitmq": {
    "client": {
      "uri": "amqp://guest:guest@localhost:5672/"
    }
  },
  "management": {
    "endpoints": {
      "path": "/cloudfoundryapplication"
    }
  }
}
